<div class="position-sticky  mt-4 mt-md-0" style="top: 2rem;">
     <div class="p-4 mb-3 bg-light shadow-sm rounded">
          <h4 class="fst-italic mb-3"><a class="style-1" target="_blank" href="/Agenda"><b>Agenda MTSs MUQ</b></a></h4>
          <% for(var index = 0; index < Object.keys(rightSideData.agenda).length; index++) {%>
          <article>
               <div class="row">
                    <div class="col-3 col-lg-2 date-agenda pt-3 pb-0 px-0">
                         <span class="d-block date-item-agenda"><%= rightSideData.agenda[index].day %></span>
                         <span class="d-block date-month-agenda"><%= rightSideData.agenda[index].month %>/<%= rightSideData.agenda[index].year %></span>
                    </div>
                    <div class="col-9 col-lg-10 pt-2 pb-1 detail-agenda">
                         <p class="mb-2"><%= rightSideData.agenda[index].agenda %></p>
                         <i class="fas fa-map-marker-alt mr-2"></i>
                         <span class="inline-block"><%= rightSideData.agenda[index].location %></span>
                    </div>
                    <hr class="m-0 w-100 line-agenda">
               </div>
          </article>
          <% } %>
     </div>
     <%- include('tentang') %>
     <%- include('pendaftaran') %>
</div>