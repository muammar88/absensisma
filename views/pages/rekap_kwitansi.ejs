<body>
    <div class="col-12" style="font-family: 'Rubik', sans-serif;width:100%;height:100%;" id="printArea">
       <div class="row">
          <div class="col-12 py-3 px-5 justify-content-center container-fluid" >
             <div class="row">
                <div class="col-12 mb-4 text-center">
                    <div class="row">
                        <div class="col-2">
                            <img src="/static/img/logo-pemerintah-aceh.png" alt="Logo"  style="opacity: .8;background-color: white;">
                        </div>
                        <div class="col-8">
                            <h3 class="mb-0">PEMERINTAH ACEH</h3>
                            <h5 class="mb-0">DINAS PENDIDIKAN ACEH</h5>
                            <h5 class="mb-0">SMA NEGERI 9 ACEH BARAT DAYA</h5>
                            <h6 class="mb-0" style="font-size: 14px;">Jalan.H.Hanafiah Sanany. Desa Kaye Aceh Kecamatan Lembah Sabil Kab. Abdya 23762</h6>
                            <h6 class="mb-0" style="font-size: 14px;">Webside: http://sman9abdya.sch.id. Email. Sman9acehbaratdaya05@Gmail.com</h6>
                        </div>
                        <div class="col-2">
                            <img src="/static/img/logo-sman9-kop.png" alt="Logo"  style="opacity: .8;background-color: white;">
                        </div>
                        <hr class="w-100 mt-3 mb-4" style="border-top: 3px solid rgb(0 0 0);">
                        <div class="col-12">
                            <h6 class="mb-1">REKAP DAFTAR HADIR GURU DAN TENDIK SMA NEGERI 9 ACEH BARAT DAYA</h6> 
                        </div>
                    </div>
                </div>
                <div class="col-4 mt-0">
                    <div class="row">
                        <div class="col-12">
                            <table class="table">
                                <tr>
                                    <td class="py-1 px-0" style="width: 30%;border: none;">Dari</td>
                                    <td class="py-1 px-0" style="width: 5%;border: none;"> : </td>
                                    <td class="py-1 px-0" style="border: none;"><%= start_date %></td>
                                </tr>
                                <tr>
                                    <td class="py-1 px-0" style="border: none;">Sampai</td>
                                    <td class="py-1 px-0" style="border: none;"> : </td>
                                    <td class="py-1 px-0" style="border: none;"><%= end_date %></td>
                                </tr>
                                <tr>
                                    <td class="py-1 px-0" style="border: none;">Total Hari</td>
                                    <td class="py-1 px-0" style="border: none;"> : </td>
                                    <td class="py-1 px-0" style="border: none;"><%= jumlah_kerja %> Hari</td>
                                </tr>
                            </table>
                        </div>
                    </div>
                </div>
                <div class="col-12 ">
                    <table class="table">
                        <thead>
                            <tr>
                                <td class="border" style="width: 3%;vertical-align: middle;" rowspan="2"><center>NO</center></td>
                                <td class="border" style="width: 20%;vertical-align: middle;" rowspan="2"><center>NAMA/NIP</center></td>
                                <td class="border" style="width: 17%;vertical-align: middle;" rowspan="2"><center>STATUS</center></td>
                                <td class="border" style="width: 60%;vertical-align: middle;" colspan="6"><center>KETERANGAN</center></td>
                                
                            </tr>
                            <tr>
                                <td class="border" style="width: 10%;vertical-align: middle;"><center>HADIR</center></td>
                                <td class="border" style="width: 10%;vertical-align: middle;"><center>IZIN</center></td>
                                <td class="border" style="width: 10%;vertical-align: middle;"><center>DL</center></td>
                                <td class="border" style="width: 10%;vertical-align: middle;"><center>SAKIT</center></td>
                                <td class="border" style="width: 10%;vertical-align: middle;"><center>CUTI HAMIL</center></td>
                                <td class="border" style="width: 10%;vertical-align: middle;"><center>TK</center></td>
                            </tr>
                        </thead>
                        <tbody>
                            <% var i = 1; %>
                            <% if ( Object.keys(list).length > 0 ) { %>
                                <% for( index in list ) { %>
                                    <tr>
                                        <td><center><%= i %></center></td>
                                        <td>
                                            <%= list[index].nama %>
                                            <br>NIP: <%= list[index].nip %>
                                            <br>Jabatan: <span style="text-transform: uppercase;font-weight: bold;"><%= list[index].jabatan %> </span>
                                        </td>
                                        <td style="text-transform: uppercase;"><center><%= list[index].status != '' ? list[index].status : '-' %></center></td>
                                        <td style="text-transform: uppercase;"><center><%= list[index].hadir != undefined ? list[index].hadir : '0' %></center></td>
                                        <td style="text-transform: uppercase;"><center><%= list[index].izin != undefined ? list[index].izin : '0' %></center></td>
                                        <td style="text-transform: uppercase;"><center><%= list[index].dinasluar != undefined ? list[index].dinasluar : '0' %></center></td>
                                        <td style="text-transform: uppercase;"><center><%= list[index].sakit != undefined ? list[index].sakit : '0' %></center></td>
                                        <td style="text-transform: uppercase;"><center><%= list[index].cuti_hamil != undefined ? list[index].cuti_hamil : '0' %></center></td>
                                        <td style="text-transform: uppercase;"><center><%= list[index].tanpa_keterangan != undefined ? list[index].tanpa_keterangan : '0' %></center></td>
                                    </tr>
                                <% i++ } %>
                            <% } else { %>
                                <tr>
                                    <td colspan="9"><center>Data absensi guru & tendik tidak ditemukan</center></td>
                                </tr>
                            <% } %>
                        </tbody>
                    </table>
                </div>
                <div class="col-8">
                </div>
                <div class="col-4 mt-5">
                    <h6 class="mb-1">Kaye Aceh, <%= tanggal %></h6> 
                    <h6 class="mb-5 pb-5">Kepala Sekolah,</h6> 
                    <h6 class="mt-5">MARZUKI, S.Pd, M.Pd</h6>
                    <h6 class="mt-0">NIP.  19800414 200504 1 001</h6>
                </div>
             </div>
          </div>
       </div>
     </div>
 </body>
